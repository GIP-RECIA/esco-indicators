<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd">

    <!--
        /properties/misc/agentUtil.xml This file declares the visual beans
        used by the web interface. It should not be modified but by the
        developers. This file is referenced by
        /properties/applicationContext.xml.
    -->

    <bean id="agentUtil" class="org.esco.indicators.web.utils.AgentUtil" scope="session">
        <property name="skins">
            <map>
                <entry key="iPhone" value="iphoneFamily" />
                <entry key="Pre" value="iphoneFamily" />
                <entry key="Android" value="iphoneFamily" />
                <entry key="BlackBerry" value="blackBerryFamily" />
            </map>
        </property>
    </bean>

</beans>