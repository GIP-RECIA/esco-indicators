<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Mapping between groups-permissions and permission rules -->
<groups-permissions>
    <group-permission>
        <match-pattern>%TYPE_ESTAB%:admin:indicateurs:central</match-pattern>
        <permission-name>%TYPE_ESTAB%</permission-name>
    </group-permission>

    <group-permission>
        <match-pattern>%TYPE_ESTAB%:admin:indicateurs:local_admin:%UAI%</match-pattern>
        <permission-name>LOCAL</permission-name>
    </group-permission>
</groups-permissions>
